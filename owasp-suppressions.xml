<?xml version="1.0" encoding="UTF-8"?>
<suppressions xmlns="https://jeremylong.github.io/DependencyCheck/dependency-suppression.1.3.xsd">
    
    <!-- Example suppressions for common false positives -->
    
    <!-- Suppress Spring Boot test dependencies -->
    <suppress>
        <notes>Spring Boot test framework - not in production</notes>
        <packageUrl regex="true">^pkg:maven/org\.springframework\.boot/spring\-boot\-starter\-test@.*$</packageUrl>
        <cve>CVE-2016-1000027</cve>
    </suppress>
    
    <!-- Suppress TestContainers vulnerabilities - test only -->
    <suppress>
        <notes>TestContainers is only used in test scope</notes>
        <packageUrl regex="true">^pkg:maven/org\.testcontainers/.*@.*$</packageUrl>
        <vulnerabilityName regex="true">.*</vulnerabilityName>
    </suppress>
    
    <!-- Suppress Kotest vulnerabilities - test only -->
    <suppress>
        <notes>Kotest is only used in test scope</notes>
        <packageUrl regex="true">^pkg:maven/io\.kotest/.*@.*$</packageUrl>
        <vulnerabilityName regex="true">.*</vulnerabilityName>
    </suppress>
    
    <!-- Example: Suppress specific CVE for a specific dependency -->
    <!--
    <suppress>
        <notes>False positive - this CVE doesn't apply to our usage</notes>
